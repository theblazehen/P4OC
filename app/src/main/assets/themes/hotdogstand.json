{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "red": "#FF0000",
    "darkRed": "#CC0000",
    "darkerRed": "#990000",
    "deepRed": "#770000",
    "bloodRed": "#550000",
    "yellow": "#FFFF00",
    "darkYellow": "#CCCC00",
    "mutedYellow": "#BBBB44",
    "white": "#FFFFFF",
    "offWhite": "#EEEECC",
    "lightGray": "#DDDDAA",
    "midGray": "#AAAAAA",
    "black": "#000000"
  },
  "theme": {
