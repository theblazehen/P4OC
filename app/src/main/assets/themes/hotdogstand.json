      "dark": "offWhite",
      "light": "offWhite"
    },
    "markdownBlockQuote": {
      "dark": "yellow",
      "light": "yellow"
    },
    "markdownListItem": {
      "dark": "white",
      "light": "white"
    },
    "markdownListEnumeration": {
      "dark": "yellow",
      "light": "yellow"
    },
    "markdownImage": {
      "dark": "yellow",
      "light": "yellow"
    },
    "markdownImageText": {
      "dark": "white",
      "light": "white"
    },
