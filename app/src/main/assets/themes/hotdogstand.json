    "backgroundPanel": {
      "dark": "darkRed",
      "light": "darkRed"
    },
    "backgroundElement": {
      "dark": "darkerRed",
      "light": "darkerRed"
    },
    "text": {
      "dark": "white",
      "light": "white"
    },
    "textMuted": {
      "dark": "offWhite",
      "light": "offWhite"
    },
    "primary": {
      "dark": "yellow",
      "light": "yellow"
    },
    "secondary": {
      "dark": "offWhite",
      "light": "offWhite"
    },
    "accent": {
      "dark": "yellow",
      "light": "yellow"
    },
